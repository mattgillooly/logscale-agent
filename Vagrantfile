# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure("2") do |config|
  config.vm.define "centos" do |cfg|
    cfg.vm.box = "centos-6.5"
    cfg.vm.box_url = "http://github.com/2creatives/vagrant-centos/releases/download/v6.5.2/centos65-x86_64-20131219.box"
  end

  config.vm.define "ubuntu" do |cfg|
    cfg.vm.box = "ubuntu"
  end
end
